# >>> fishros initialize >>>
source /opt/ros/noetic/setup.bash
# <<< fishros initialize <<<


get_ros_keywords() {
    # extract $ros_package_path
    echo "$ROS_PACKAGE_PATH" | tr ':' '\n' | awk -F'/' '{print $(NF-1)}' 2>/dev/null | paste -sd ";" -
}
export PS1='\[\e[32m\]\u@\h\[\e[34m\][$(get_ros_keywords)]\[\e[0m\]:\w\$ '

